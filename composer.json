{
  "name": "putheakhem/khmer-number-to-text",
  "description": "Convert Khmer numbers to Khmer text representation",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Puthea Khem",
      "email": "puthea.khem@gmail.com"
    }
  ],
  "require": {
    "php": "^8.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^10"
  },
  "autoload": {
    "psr-4": {
      "Putheakhem\\KhmerNumberToText\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Putheakhem\\KhmerNumberToText\\KhmerNumberToTextServiceProvider"
      ],
      "aliases": {
        "KhmerNumber": "Putheakhem\\KhmerNumberToText\\Facades\\KhmerNumber"
      }
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
